{
    "applications": {
        "gecko": {
            "id": "kekos91@free.fr",
            "strict_min_version": "42.0"
        }
    },
    "manifest_version": 2,
    "name": "TOO MUCH CAPS",
    "version": "1.0",
    "description": "Make every youtube title non-caps, because we're pissed about youtubers writing onlyy with capslock cause they don't get any views without it.",
    "icons": {
        "48": "icons/toomuchcaps-48.png"
    },
    "content_scripts": [{
        "matches": ["*://*.youtube.com/*"],
        "js": ["toomuchcaps.js"],
        "run_at": "document_start"
    }]
}
